{
  "include": ["**/*.ts"],
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "baseUrl": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
